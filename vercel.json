{
  "version": 2,
  "builds": [
    { "src": "*.{jpg,jpeg,png,webp}", "use": "@vercel/static" },
    { "src": "photos/**", "use": "@vercel/static" },
    { "src": "Reviews/**", "use": "@vercel/static" },
    { "src": "blog/**", "use": "@vercel/static" },
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}
